{
    "name": "backend",
    "version": "1.0.0",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "node --experimental-transform-types --env-file=.env --watch .\\src\\index.ts",
        "start": "node --experimental-transform-types --env-file=.env .\\src\\index.ts",
        "typecheck": "tsc",
        "test": "jest --runInBand",
        "test:watch": "jest --watch --runInBand",
        "test:coverage": "jest --coverage --runInBand",
        "prisma:generate": "prisma generate && prisma generate --sql",
        "prisma:push": "prisma db push",
        "prisma:studio": "prisma studio",
        "prisma:seed": "prisma db seed",
        "prisma:seed:perf": "npx ts-node prisma/seed-performance.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.3.1",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.0",
        "json-2-csv": "^5.5.9",
        "jsonwebtoken": "^9.0.2",
        "zod": "^3.24.4"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/express-serve-static-core": "^5.0.7",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.3",
        "@types/node": "^20.8.2",
        "jest": "^29.7.0",
        "prisma": "^5.3.1",
        "ts-jest": "^29.3.4",
        "typescript": "^5.2.2"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
